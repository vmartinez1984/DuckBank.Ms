{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "Enabled": true,
      "MinimumLevel": "Information"
    },
    "Graylog": {
      "Enabled": true,
      "Host": "192.168.1.86",
      "Port": "12201",
      "MinimumLevel": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "mongoDb": "",
    "mongoDbNombre": "DuckBank_Produccion",
    "mongoDbLogs": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "MongoDBBson",
        "Args": {
          "databaseUrl": "",
          "collectionName": "Exceptions",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000"
        }
      }
    ]
  }
}
